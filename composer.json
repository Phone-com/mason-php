{
  "name": "phonecom/mason-php",
  "description": "PHP library for generating Mason hypermedia documents",
  "license": "MIT",
  "keywords": ["mason", "hypermedia", "rest"],
  "support": {
    "issues": "https://github.com/Phone-com/mason-php/issues",
    "source": "https://github.com/Phone-com/mason-php"
  },
  "authors": [
    {
      "name": "Curtis Farnham",
      "email": "cfarnham@phone.com"
    }
  ],
  "require": {
    "php": ">=5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "squizlabs/php_codesniffer": "2.*",
    "justinrainbow/json-schema": "^1.4"
